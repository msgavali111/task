<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Accordion"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <nestedMultifieldWithMap
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Nested Multifield with Map"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                <items jcr:primaryType="nt:unstructured">
                                    <parentItemWithMap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Book Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./multifieldItems">
                                            <items jcr:primaryType="nt:unstructured">
                                                <authorName
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Author Name"
                                                    name="./authorName"/>
                                                <nestedItemWithMap
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Nested Book Details">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./nestedMultifieldItemsWIthMap">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <bookName
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Book Name"
                                                                name="./bookName"/>
                                                            <publishDate
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                                fieldLabel="Publish Date"
                                                                name="./publishDate"/>
                                                        </items>
                                                    </field>
                                                </nestedItemWithMap>
                                            </items>
                                        </field>
                                    </parentItemWithMap>
                                </items>
                            </well>
                        </items>
                    </nestedMultifieldWithMap>
                    <nestedMultifieldWithBean
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Nested Multifield with Bean"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                <items jcr:primaryType="nt:unstructured">
                                    <nestedItemWithBean
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Address Items">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./nestedBookDetailsBean">
                                            <items jcr:primaryType="nt:unstructured">
                                                <bookName
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Book Name"
                                                    name="./bookName"/>
                                                <bookPublishDate
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                    fieldLabel="Book Publish Date"
                                                    name="./bookPublishDate"/>
                                                <bookCopies
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                    fieldLabel="Book Copies"
                                                    name="./bookCopies"/>
                                            </items>
                                        </field>
                                    </nestedItemWithBean>
                                </items>
                            </well>
                        </items>
                    </nestedMultifieldWithBean>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
